<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>sh.brew.update</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Users/frederic/.local/bin/brew-auto-update</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/opt/homebrew/var/log/brew-auto-update.err</string>
	<key>StandardOutPath</key>
	<string>/opt/homebrew/var/log/brew-auto-update.log</string>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Hour</key>
			<integer>12</integer>
			<key>Minute</key>
			<integer>0</integer>
		</dict>
	</array>
</dict>
</plist>
